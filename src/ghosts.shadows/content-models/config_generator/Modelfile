        },
        {
            "HandlerType": "Command",
            "Initial": "",
            "UtcTimeOn": "00:00:00",
            "UtcTimeOff": "24:00:00",
            "Loop": true,
            "TimeLineEvents": [
                {
                    "Command": "<command_name>",
